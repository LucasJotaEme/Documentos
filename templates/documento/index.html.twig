{% extends 'base.html.twig' %}

{% block title %}Documentos UNRaf{% endblock %}

{% block body %}
<style>
    .example-wrapper { margin: 1em auto; max-width: 800px; width: 95%; font: 18px/1.5 sans-serif; }
    .example-wrapper code { background: #F5F5F5; padding: 2px 6px; }
</style>

{{ parent() }}

<div>
    {{ form_start(formulario, {'enctype':'multipart/form-data'})}}
    
    {{ form_end(formulario) }}
</div>
<div class="">
    <a href="{{path('documentosObsoletos')}}">Documentos obsoletos</a>
    {% for documento in documentos %}
        <div>
            <h2>{{documento.numero}}</h2>
            <a href="{{ path('modificarDocumento', {'id': documento.id}) }}">Modificar</a>
            <a href="{{ path('eliminarDocumento', {'id': documento.id}) }}">Eliminar</a>
        </div>
        
    {% endfor %}
</div>
{% endblock %}
